{
  "name": "ngx-daterangepicker-material",
  "version": "2.1.10",
  "description": "Angular 2+ date range picker (with material design theme)",
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "ng2",
    "daterangepicker",
    "material",
    "component",
    "directive",
    "daterange",
    "date",
    "moment",
    "picker"
  ],
  "homepage": "https://github.com/fetrarij/ngx-daterangepicker-material",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "demo:build": "ng build --prod",
    "test": "./node_modules/.bin/ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
    "gh-pages": "ng build --prod --base-href=\"/ngx-daterangepicker-material/\" && ngh --dir demo/dist",
    "build": "ng-packagr -p package.json",
    "format:check": "prettier \"./{src,cypress}/**/*.{scss,ts,html,json,md}\"",
    "format:write": "prettier --write \"./{src,cypress}/**/*.{scss,ts,html,json,md}\""
  },
  "author": "Fetrarijaona R.",
  "repository": {
    "type": "git",
    "url": "https://github.com/fetrarij/ngx-daterangepicker-material.git"
  },
  "peerDependencies": {
    "moment": "^2.21.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "8.3.21",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.1.2",
    "angular-cli-ghpages": "^0.6.1",
    "codelyzer": "^5.0.1",
    "core-js": "^3.6.1",
    "materialize-css": "^1.0.0",
    "moment": "^2.22.2",
    "ng-packagr": "^5.4.0",
    "prettier": "1.19.1",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "ts-node": "~8.5.4",
    "tslint": "~5.20.1",
    "typescript": "3.5.3",
    "zone.js": "0.9.1"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/daterangepicker/index.ts",
      "umdModuleIds": {
        "moment": "moment"
      }
    },
    "dest": "dist"
  },
  "dependencies": {
    "tslib": "^1.9.0"
  }
}
